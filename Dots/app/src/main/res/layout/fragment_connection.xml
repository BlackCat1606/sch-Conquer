    <ImageView
            android:layout_width="538dp"
            android:layout_height="716dp"
            android:src="@drawable/conquermain"
            android:layout_gravity="center"/>

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="8"
            android:id="@+id/ipAddress"
            android:layout_gravity="center"
            android:layout_marginBottom="15dp"
            android:layout_marginLeft="5dp"
            android:textColor="#ffffff"/>

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/startClientButton"
        android:src="@drawable/rocket1"
        android:layout_gravity="center_horizontal|bottom"
        android:layout_marginBottom="100dp"
        android:layout_marginLeft="-50dp"
        android:background="@android:color/transparent"/>

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/startServerButton"
        android:src="@drawable/rocket2"
        android:background="@android:color/transparent"
        android:layout_gravity="center_horizontal|bottom"
        android:layout_marginBottom="100dp"
        android:layout_marginLeft="50dp"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/targetLocation"
            android:text="@string/targetLocation"
            android:layout_marginTop="-50dp"
            android:textColor="#ffffff"
            android:layout_gravity="center"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/currentLocation"
            android:text="@string/currentLocation"
            android:layout_marginTop="80dp"
            android:textColor="#ffffff"
            android:layout_gravity="center_horizontal|top"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/player_ip_address"
        android:text="@string/My_IP_Address"
        android:textColor="#ffffff"
        android:layout_marginTop="130dp"
        android:layout_gravity="center_horizontal|top"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/chooseRocket"
            android:text="@string/chooseRocket"
            android:layout_marginTop="50dp"
            android:textColor="#ffffff"
            android:layout_gravity="center"/>


</FrameLayout>
